<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<link
			rel="stylesheet"
			href="/stylesheets/style.css" />
	</head>
	<body>
		<h1><%= title %></h1>
		<ol>
			<% products.forEach(product => { %>
			<li><%= product.title%> - price: <%= product.price %></li>
			<% }) %>
		</ol>

		<table
			border="1"
			cellpadding="8"
			cellspacing="0">
			<thead>
				<tr>
					<th>ID</th>
					<th>Title</th>
					<th>Price</th>
					<th>Category</th>
				</tr>
			</thead>
			<tbody>
				<% products.forEach(product => { %>
				<tr>
					<td><%= product.id %></td>
					<td><%= product.title %></td>
					<td><%= product.price %> грн</td>
					<td><%= product.category %></td>
				</tr>
				<% }) %>
			</tbody>
		</table>
	</body>
</html>
